
public class example21 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=90;
		int b=80;
		int c=70;
		int d=60;
		if(a>=90)
		{
			System.out.println("A");
		}
		else if(b>=80&&b<90)
		{
			System.out.println("B");
		}
		else if(c>=70&&b<80)
		{
			System.out.println("C");
		}
		else if(d>=60&&b<70)
		{
			System.out.println("D");
		}
		else
		{
			System.out.println("F");
		}
	}

}
