package day0310;

public class varex2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char c1 = 'a';
		System.out.println(c1);
		char c3 = (char)(c1+1);
		char c4 = ++c1;
		int i = 'B'-'A';
		int j = '2'-'0';
		System.out.println(c3);
		System.out.println(c4);
		System.out.println(i);
		System.out.println(j);
		System.out.println(20>>2);
		
	}

}
