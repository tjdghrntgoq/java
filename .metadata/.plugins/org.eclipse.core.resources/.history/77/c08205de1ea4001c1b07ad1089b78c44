package day0315;

public class example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//socrer가 68이다.
		//socore가 60이상이면 합격을 출력하고
		//그렇지 않으면 불합격을 출력 하시오
		int score=68;
		if (score >= 60)
		{
			System.out.println("합격");
		}
		else (score<60)
		{
			System.out.println("불합격");
		}
	}

}
