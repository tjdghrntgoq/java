package dya0314;

public class qweqwe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean android =true;
		boolean ios =false;
		//만약에 안드로이드가 true 라면
		if (android==true)
		{
			System.out.println("안드로이드 입니다.");
		}
		else
		{
			ios=(false);
			System.out.println("ios 입니다.");
		}
	}

}
